{
  "name": "@itriton/webpack",
  "type": "module",
  "version": "0.0.9",
  "description": "",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./index.d.ts"
    }
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "test": "pnpm jest",
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "deploy:git": "GH=1 pnpm run docs && bash scripts/deploy-git.sh",
    "deploy:server": "GH=1 pnpm run docs && bash scripts/deploy-server.sh",
    "docs": "typedoc --options typedoc.json",
    "version patch": "pnpm version patch -m '🚀 发布修订版'",
    "version minor": "pnpm version minor -m '🚀 发布新功能'",
    "version major": "pnpm version major -m '🚀 发布新版本'",
    "build:publish": "pnpm publish --access=public"
  },
  "keywords": [
    "JavaScript",
    "JS",
    "@itriton/webpack",
    "@itriton",
    "webpack",
    "工具",
    "Tools"
  ],
  "author": "icjc-cc",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@types/cssbeautify": "^0.3.5",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.3",
    "babel-jest": "^29.5.0",
    "babel-preset-env": "^1.7.0",
    "esbuild": "^0.17.18",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "rollup": "^3.23.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.6",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "cssbeautify": "^0.3.1",
    "tslib": "^2.6.2"
  }
}
